	  (Golgi_DeSouza10_nemo_neuron_netstim_run
	   (label . "Golgi DeSouza 2010")
	   (config-path  . ,(make-pathname model-source-dir "Golgi_DeSouza10_nemo_neuron_netstim_run"))
